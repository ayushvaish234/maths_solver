<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".quadratic">

        <ImageButton
            android:id="@+id/back3"
            android:layout_width="29dp"
            android:layout_height="35dp"
            android:backgroundTint="#00FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.013"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.022"
            app:srcCompat="@drawable/back"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/a"
            android:layout_width="50dp"
            android:layout_height="81dp"

            android:ems="10"
            android:fontFamily="@font/cantora_one"
            android:hint="a"
            android:inputType="numberDecimal|numberSigned"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.231" />

        <EditText
            android:id="@+id/c"
            android:layout_width="50dp"
            android:layout_height="81dp"

            android:ems="10"
            android:fontFamily="@font/cantora_one"
            android:hint="c"
            android:inputType="numberDecimal|numberSigned"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.99"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.233" />

        <EditText
            android:id="@+id/b"
            android:layout_width="50dp"
            android:layout_height="81dp"

            android:ems="10"
            android:fontFamily="@font/cantora_one"
            android:hint="b"
            android:inputType="numberDecimal|numberSigned"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.531"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.232" />

        <Button
            android:id="@+id/generate"
            android:layout_width="93dp"
            android:layout_height="57dp"
            android:backgroundTint="#3A98B9"
            android:text="="
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.379" />

        <TextView
            android:id="@+id/res"
            android:layout_width="308dp"
            android:layout_height="114dp"

            android:layout_marginStart="16dp"
            android:layout_marginEnd="87dp"
            android:layout_marginBottom="168dp"
            android:fontFamily="@font/cantora_one"
            android:gravity="center"
            android:paddingLeft="5sp"
            android:textColor="@color/pink"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/res2" />

        <TextView
            android:id="@+id/res2"
            android:layout_width="308dp"
            android:layout_height="114dp"

            android:layout_marginStart="16dp"
            android:layout_marginTop="400dp"
            android:layout_marginEnd="87dp"
            android:layout_marginBottom="285dp"
            android:fontFamily="@font/cantora_one"
            android:gravity="center"
            android:paddingLeft="5sp"
            android:textColor="@color/pink"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="78dp"
            android:layout_height="81dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="167dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="151dp"
            android:gravity="center|clip_vertical"
            android:text="x+"
            android:textColor="#1E88E5"
            android:textSize="62sp"
            app:layout_constraintBottom_toTopOf="@+id/res2"
            app:layout_constraintEnd_toStartOf="@+id/c"
            app:layout_constraintHorizontal_bias="0.65"
            app:layout_constraintStart_toEndOf="@+id/b"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="91dp"
            android:layout_height="81dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="167dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="33dp"
            android:gravity="clip_vertical"
            android:text="xÂ²+"
            android:textColor="#1E88E5"
            android:textSize="62sp"
            app:layout_constraintBottom_toTopOf="@+id/generate"
            app:layout_constraintEnd_toStartOf="@+id/b"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/a"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="226dp"
            android:layout_height="40dp"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="Quadratic solver"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>